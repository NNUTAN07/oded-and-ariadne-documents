az aks create \
    --resource-group myResourceGroup \
    --name myAKSCluster \
    --node-count 3 \
    --node-vm-size Standard_DS2_v2 \
    --node-osdisk-size 30 \
    --zones 1 2

https://learn.microsoft.com/en-us/cli/azure/aks?view=azure-cli-latest
https://learn.microsoft.com/en-us/azure/aks/quotas-skus-regions
